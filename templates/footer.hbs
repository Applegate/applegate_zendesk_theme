<script>
  var Public = (function() {
    var app = {
      init: function() {
        navigation.init();
        this.setSocialNavWidth();
        this.onResize();
      },
      setSocialNavWidth: function() {
        var socialNavWidth = $('.js-nav-social').outerWidth();
        $('.js-nav-social ul').css('width', socialNavWidth + 'px');
      },
      isMobile: function() {
        if ($(window).outerWidth() <= 768) {
          return true;
        } else {
          return false;
        }
      },
      onResize: function() {
        var self = this;
        $(window).on('resize', function(e) {
          self.setSocialNavWidth();
        });
      }
    };
    var navigation = {
      init: function() {
        this.navigationDropdown();
        this.bindMobileNavigation();
      },
      bindMobileNavigation: function() {
        var self = this;
        $('.js-mobile-nav').on('click', function() {
          self.triggerSideCanvas(this);
        });
      },
      triggerSideCanvas: function(el) {
        if (app.isMobile()) {
          $('.wrap--outer').toggleClass('-extended');
          this.scrollToTop();
          // if data-side-canvas is set, add the value as a css class to .wrap-outer
          if ($(el).data('side-canvas')) {
            $('.wrap--outer').css('height', $('.mobile--side-canvas').outerHeight());
            $('.wrap--outer').addClass('-' + $(el).data('side-canvas'));
          } else {
          // remove all classes after animation ends
            setTimeout(function() {
              $('.wrap--outer').css('height', '');
              $('.wrap--outer').attr('class', 'wrap--outer');
            }, 600);
          }
        }
      },

      scrollToTop: function() {
      $('.wrap--outer').scrollTop(0);
      },

      navigationDropdown: function() {
        var delay;
        $('.js-nav-dropdown').hover(
          function(event) {
            var self = this;
            delay = setTimeout(function() {
              var targetDropdown = $('.' + $(self).attr('data-target'));
              $(self).addClass('toggled');
              targetDropdown.fadeIn(150);
            }, 300);
          },
          function(event) {
            var targetDropdown = $('.' + $(this).attr('data-target'));
            if (!$(event.relatedTarget).is(targetDropdown)) {
              $(this).removeClass('toggled');
              targetDropdown.fadeOut(150);
              clearTimeout(delay);
            }
          }
        );
        $('.navigation--dropdown').on('mouseleave', function(event) {
          // to prevent closing dropdown when ancestor of relatedTarget is .js-nav-dropdown
          if (!$(event.relatedTarget).closest('.toggled').length) {
            $('.toggled').removeClass('toggled');
            $(this).fadeOut(150);
          }
        });
      }
    };
    return {
      app: app,
      navigation: navigation,
    };
  })();
  $(function() {
  Public.app.init();
  });
</script>
